<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<navigation-rule>
		<from-view-id>/login/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loginSuccessful</from-outcome>
			<to-view-id>/defaultPage.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>loginUnSuccessful</from-outcome>
			<to-view-id>/login/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/customer/addCustomer.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addCustomerResult</from-outcome>
			<to-view-id>/customer/addCustomerResult.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
